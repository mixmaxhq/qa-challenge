		{
			"name": "Create pet with valid data",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.environment.set(\"petId\", pm.response.json().id);\r",
							"\r",
							"pm.test(\"Status code is '200'\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Category is 'Dog'\", function () {\r",
							"    pm.expect(pm.response.json().category.name).to.eql(\"Dog\");\r",
							"});\r",
							"\r",
							"pm.test(\"Name is 'Max'\", function () {\r",
							"    pm.expect(pm.response.json().name).to.eql(\"Max\");\r",
							"});\r",
							"\r",
							"pm.test(\"Status is 'available'\", function () {\r",
							"    pm.expect(pm.response.json().status).to.eql(\"available\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"id\": 123456789,\r\n  \"category\": {\r\n    \"id\": 10,\r\n    \"name\": \"Dog\"\r\n  },\r\n  \"name\": \"Max\",\r\n  \"photoUrls\": [\"https://cdn.shopify.com/s/files/1/0284/2391/3547/articles/American-Pitbull-Puppies-6.JPG_c4357295-9ee4-45f8-9718-e36c98022c46.jpg?v=1603063314\"],\r\n  \"status\": \"available\"\r\n}",
